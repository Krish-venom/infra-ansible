[apache]
%{ for ip in apache_servers ~}
${ip}
%{ endfor ~}

[nginx]
%{ for ip in nginx_servers ~}
${ip}
%{ endfor ~}

# Group variables (applied automatically by Ansible to each host in the group)
[apache:vars]
ansible_user=${ansible_user}
ansible_ssh_private_key_file=../keys/${keypair_name}.pem
ansible_python_interpreter=/usr/bin/python3

[nginx:vars]
ansible_user=${ansible_user}
ansible_ssh_private_key_file=../keys/${keypair_name}.pem
ansible_python_interpreter=/usr/bin/python3
